<if test="model.regdate neq null">and to_char(m.REGDATE,'YYYY-MM-DD') = to_char(#{model.regdate,jdbcType=DATE javaType=Date },'YYYY-MM-DD') </if> 
<if test="model.ccontent neq null">and m.C_CONTENT = #{model.ccontent,jdbcType=VARCHAR javaType=String } </if> 
<if test="model.cdes1 neq null">and m.C_DES1 = #{model.cdes1,jdbcType=VARCHAR javaType=String } </if> 
