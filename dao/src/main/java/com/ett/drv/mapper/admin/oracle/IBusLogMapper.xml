<sql id="table">TABLE_BUSLOGS</sql>

<sql id="pk">ID</sql>

<sql id="colums">
ID,C_OPERATOR,C_BUSTYPE,REGDATE,C_CONTENT,C_DES1,C_DES2,C_DES3,I_DEPID 
</sql>

<sql id="insertCols">
