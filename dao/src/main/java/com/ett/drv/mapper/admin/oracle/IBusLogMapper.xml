#{COperator,jdbcType=VARCHAR,javaType=String},
#{CBustype,jdbcType=VARCHAR,javaType=String},
#{Regdate,jdbcType=DATE,javaType=Date},
#{CContent,jdbcType=VARCHAR,javaType=String},
#{CDes1,jdbcType=VARCHAR,javaType=String},
#{CDes2,jdbcType=VARCHAR,javaType=String},
#{CDes3,jdbcType=VARCHAR,javaType=String},
#{IDepid,jdbcType=INTEGER,javaType=Integer},

</sql>

<sql id="updateCols">
