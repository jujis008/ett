<sql id="selectCondition">
<if test="model.Id neq null">and m.ID = #{model.Id,jdbcType=INTEGER javaType=Integer } </if> 
<if test="model.COperator neq null">and m.C_OPERATOR like #{model.COperator,jdbcType=VARCHAR javaType=String } </if> 
<if test="model.CBustype neq null">and m.C_BUSTYPE like #{model.CBustype,jdbcType=VARCHAR javaType=String } </if> 
<if test="model.Regdate neq null">and to_char(m.REGDATE,'YYYY-MM-DD') = to_char(#{model.Regdate,jdbcType=DATE javaType=Date },'YYYY-MM-DD') </if> 
<if test="model.CContent neq null">and m.C_CONTENT like #{model.CContent,jdbcType=VARCHAR javaType=String } </if> 
<if test="model.CDes1 neq null">and m.C_DES1 like #{model.CDes1,jdbcType=VARCHAR javaType=String } </if> 
<if test="model.CDes2 neq null">and m.C_DES2 like #{model.CDes2,jdbcType=VARCHAR javaType=String } </if> 
<if test="model.CDes3 neq null">and m.C_DES3 like #{model.CDes3,jdbcType=VARCHAR javaType=String } </if> 
<if test="model.IDepid neq null">and m.I_DEPID = #{model.IDepid,jdbcType=INTEGER javaType=Integer } </if> 
<if test="model.qStartRegDate neq null and model.qEndRegDate neq null">
  and (m.REGDATE between #{model.qStartRegDate,jdbcType=DATE javaType=Date }  and  #{model.qEndRegDate,jdbcType=DATE javaType=Date } ) </if> 
</sql>


<select id="select" resultType="ArrayList" resultMap="resultMap">
	select m.* from <include refid="table" /> m   
		<trim prefix="where 1=2 or (" suffix=")" prefixOverrides="and|or" suffixOverrides="and|or">
			<include refid="selectCondition"/>
