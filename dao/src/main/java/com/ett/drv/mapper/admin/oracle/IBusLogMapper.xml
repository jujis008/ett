C_OPERATOR=#{model.COperator,jdbcType=VARCHAR,javaType=String},
C_BUSTYPE=#{model.CBustype,jdbcType=VARCHAR,javaType=String},
REGDATE=#{model.Regdate,jdbcType=DATE,javaType=Date},
C_CONTENT=#{model.CContent,jdbcType=VARCHAR,javaType=String},
C_DES1=#{model.CDes1,jdbcType=VARCHAR,javaType=String},
C_DES2=#{model.CDes2,jdbcType=VARCHAR,javaType=String},
C_DES3=#{model.CDes3,jdbcType=VARCHAR,javaType=String},
