<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.ett.drv.mapper.admin.IDepartmentMapper">

<cache flushInterval="30000" readOnly="true"></cache>


<sql id="table">TABLE_DEPARTMENTS</sql>

<sql id="pk">ID</sql>

<sql id="colums">
ID,C_CONNECTOR,C_MOBILE,C_PHONE,C_COMPANYCODE,C_DEPFULLNAME,C_DEPNICKNAME,C_DEPCODE,C_PARENTCODE,C_DEPTYPE 
</sql>

<sql id="insertCols">
#{id,jdbcType=INTEGER,javaType=Integer},
#{connector,jdbcType=VARCHAR,javaType=String},
#{mobile,jdbcType=VARCHAR,javaType=String},
#{phone,jdbcType=VARCHAR,javaType=String},
#{companycode,jdbcType=VARCHAR,javaType=String},
#{depfullname,jdbcType=VARCHAR,javaType=String},
#{depnickname,jdbcType=VARCHAR,javaType=String},
#{depcode,jdbcType=VARCHAR,javaType=String},
#{parentcode,jdbcType=VARCHAR,javaType=String},
#{deptype,jdbcType=VARCHAR,javaType=String} 
</sql>

<sql id="updateCols">
 <if test="pk neq null">ID=#{pk},</if>
C_CONNECTOR=#{model.connector,jdbcType=VARCHAR,javaType=String},
C_MOBILE=#{model.mobile,jdbcType=VARCHAR,javaType=String},
C_PHONE=#{model.phone,jdbcType=VARCHAR,javaType=String},
C_COMPANYCODE=#{model.companycode,jdbcType=VARCHAR,javaType=String},
C_DEPFULLNAME=#{model.depfullname,jdbcType=VARCHAR,javaType=String},
C_DEPNICKNAME=#{model.depnickname,jdbcType=VARCHAR,javaType=String},
C_DEPCODE=#{model.depcode,jdbcType=VARCHAR,javaType=String},
C_PARENTCODE=#{model.parentcode,jdbcType=VARCHAR,javaType=String},
C_DEPTYPE=#{model.deptype,jdbcType=VARCHAR,javaType=String}
</sql>

<sql id="orderby">order by ID</sql>

<sql id="joinColums"></sql>

<sql id="join"></sql><!-- 别名m已被主表使用  -->

<resultMap type="com.ett.drv.model.admin.DepartmentModel" id="resultMap">
 <id column="ID"  property="id" jdbcType="INTEGER" javaType="Integer"/>
<result column="C_CONNECTOR"  property="connector" jdbcType="VARCHAR" javaType="String"/>
<result column="C_MOBILE"  property="mobile" jdbcType="VARCHAR" javaType="String"/>
<result column="C_PHONE"  property="phone" jdbcType="VARCHAR" javaType="String"/>
<result column="C_COMPANYCODE"  property="companycode" jdbcType="VARCHAR" javaType="String"/>
<result column="C_DEPFULLNAME"  property="depfullname" jdbcType="VARCHAR" javaType="String"/>
<result column="C_DEPNICKNAME"  property="depnickname" jdbcType="VARCHAR" javaType="String"/>
<result column="C_DEPCODE"  property="depcode" jdbcType="VARCHAR" javaType="String"/>
<result column="C_PARENTCODE"  property="parentcode" jdbcType="VARCHAR" javaType="String"/>
<result column="C_DEPTYPE"  property="deptype" jdbcType="VARCHAR" javaType="String"/>
 </resultMap>


<select id="select" resultType="ArrayList" resultMap="resultMap">
	 select m.* <include refid="joinColums"/> from <include refid="table" /> m <include refid="join"/> 
<trim prefix="where" prefixOverrides="and">
<if test="model.id neq null">and m.ID=#{model.id,jdbcType=INTEGER javaType=Integer } </if> 
<if test="model.connector neq null">and m.C_CONNECTOR=#{model.connector,jdbcType=VARCHAR javaType=String } </if> 
<if test="model.mobile neq null">and m.C_MOBILE=#{model.mobile,jdbcType=VARCHAR javaType=String } </if> 
<if test="model.phone neq null">and m.C_PHONE=#{model.phone,jdbcType=VARCHAR javaType=String } </if> 
<if test="model.companycode neq null">and m.C_COMPANYCODE=#{model.companycode,jdbcType=VARCHAR javaType=String } </if> 
<if test="model.depfullname neq null">and m.C_DEPFULLNAME=#{model.depfullname,jdbcType=VARCHAR javaType=String } </if> 
<if test="model.depnickname neq null">and m.C_DEPNICKNAME=#{model.depnickname,jdbcType=VARCHAR javaType=String } </if> 
<if test="model.depcode neq null">and m.C_DEPCODE=#{model.depcode,jdbcType=VARCHAR javaType=String } </if> 
<if test="model.parentcode neq null">and m.C_PARENTCODE=#{model.parentcode,jdbcType=VARCHAR javaType=String } </if> 
<if test="model.deptype neq null">and m.C_DEPTYPE=#{model.deptype,jdbcType=VARCHAR javaType=String } </if> 
</trim>
</select>
<select id="selectEqPk" parameterType="String"  resultMap="resultMap">
	 select <include refid="colums"/> from <include refid="table" /> m <include refid="join"/> where m.<include refid="pk" /> = #{id,jdbcType=INTEGER,javaType=Integer}
</select>

<select id="count" resultType="int">
	 select count(*) from <include refid="table"/>
</select>

<select id="selectInPk" resultType="ArrayList" resultMap="resultMap" >
	 select m.* <include refid="joinColums"/> from <include refid="table" /> m <include refid="join"/> where m.<include refid="pk" /> in
<foreach item="pk" collection="list" open="(" separator="," close=")"> #{pk} </foreach> 
<include refid="orderby"/>
</select>

<select id="selectNotInPk" resultType="ArrayList" resultMap="resultMap" >
	 select m.* <include refid="joinColums"/> from <include refid="table" /> m <include refid="join"/> where m.<include refid="pk" /> not in
<foreach item="pk" collection="list" open="(" separator="," close=")"> #{pk} </foreach> 
<include refid="orderby"/>
</select>

<select  id="selectAll"  resultType="ArrayList"  resultMap="resultMap">
	 select m.* <include refid="joinColums"/> from  <include refid="table" /> m <include refid="join"/> 
 <include refid="orderby"/>
</select>

<insert id="insertOne" >
	 insert into <include refid="table"/> ( <include refid="colums"/> ) values ( <include refid="insertCols"/>) 
</insert>

<update id="updateOne" flushCache="true" >
	 update <include refid="table"/> set <include refid="updateCols"/> where <include refid="pk" />=#{model.id,jdbcType=INTEGER,javaType=Integer}
</update>

<delete id="deleteInPk" flushCache="true">
	 delete from <include refid="table"/> where <include refid="pk" /> in
<foreach item="pk" collection="list" open="(" separator="," close=")">#{pk}</foreach>
</delete>

<delete id="deleteNotInPk" flushCache="true">
	 delete from <include refid="table"/> where <include refid="pk" /> not in
<foreach item="pk" collection="list" open="(" separator="," close=")">#{pk}</foreach>
</delete>



</mapper>
