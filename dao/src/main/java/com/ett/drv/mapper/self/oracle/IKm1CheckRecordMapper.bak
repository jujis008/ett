<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.ett.drv.mapper.self.IKm1CheckRecordMapper">

<cache flushInterval="30000" readOnly="true"></cache>


<sql id="table">TABLE_SELF_KM1_CHECK_RECORD</sql>

<sql id="pk">ID</sql>

<sql id="colums">
ID,C_CREATEIP,T_CREATETIME,C_CREATOR,I_ISDELETED,C_DESCRIPTION,C_KEYWORDS,C_MODIFIER,C_MODIFYIP,T_MODIFYTIME,KSDD,
C_KSDDNAME,LSH,SFZMHM,XM 
</sql>

<sql id="insertCols">
#{id,jdbcType=INTEGER,javaType=Integer},
#{createip,jdbcType=VARCHAR,javaType=String},
#{createtime,jdbcType=DATE,javaType=Date},
#{creator,jdbcType=VARCHAR,javaType=String},
#{isdeleted,jdbcType=INTEGER,javaType=Integer},
#{description,jdbcType=VARCHAR,javaType=String},
#{keywords,jdbcType=VARCHAR,javaType=String},
#{modifier,jdbcType=VARCHAR,javaType=String},
#{modifyip,jdbcType=VARCHAR,javaType=String},
#{modifytime,jdbcType=DATE,javaType=Date},
#{ksdd,jdbcType=VARCHAR,javaType=String},
#{ksddname,jdbcType=VARCHAR,javaType=String},
#{lsh,jdbcType=VARCHAR,javaType=String},
#{sfzmhm,jdbcType=VARCHAR,javaType=String},
#{xm,jdbcType=VARCHAR,javaType=String} 
</sql>

<sql id="updateCols">
 <if test="pk neq null">ID=#{pk},</if>
C_CREATEIP=#{model.createip,jdbcType=VARCHAR,javaType=String},
T_CREATETIME=#{model.createtime,jdbcType=DATE,javaType=Date},
C_CREATOR=#{model.creator,jdbcType=VARCHAR,javaType=String},
I_ISDELETED=#{model.isdeleted,jdbcType=INTEGER,javaType=Integer},
C_DESCRIPTION=#{model.description,jdbcType=VARCHAR,javaType=String},
C_KEYWORDS=#{model.keywords,jdbcType=VARCHAR,javaType=String},
C_MODIFIER=#{model.modifier,jdbcType=VARCHAR,javaType=String},
C_MODIFYIP=#{model.modifyip,jdbcType=VARCHAR,javaType=String},
T_MODIFYTIME=#{model.modifytime,jdbcType=DATE,javaType=Date},
KSDD=#{model.ksdd,jdbcType=VARCHAR,javaType=String},
C_KSDDNAME=#{model.ksddname,jdbcType=VARCHAR,javaType=String},
LSH=#{model.lsh,jdbcType=VARCHAR,javaType=String},
SFZMHM=#{model.sfzmhm,jdbcType=VARCHAR,javaType=String},
XM=#{model.xm,jdbcType=VARCHAR,javaType=String}
</sql>

<sql id="orderby">order by ID</sql>

<sql id="joinColums"></sql>

<sql id="join"></sql><!-- 别名m已被主表使用  -->

<resultMap type="com.ett.drv.model.self.Km1CheckRecordModel" id="resultMap">
 <id column="ID"  property="id" jdbcType="INTEGER" javaType="Integer"/>
<result column="C_CREATEIP"  property="createip" jdbcType="VARCHAR" javaType="String"/>
<result column="T_CREATETIME"  property="createtime" jdbcType="DATE" javaType="Date"/>
<result column="C_CREATOR"  property="creator" jdbcType="VARCHAR" javaType="String"/>
<result column="I_ISDELETED"  property="isdeleted" jdbcType="INTEGER" javaType="Integer"/>
<result column="C_DESCRIPTION"  property="description" jdbcType="VARCHAR" javaType="String"/>
<result column="C_KEYWORDS"  property="keywords" jdbcType="VARCHAR" javaType="String"/>
<result column="C_MODIFIER"  property="modifier" jdbcType="VARCHAR" javaType="String"/>
<result column="C_MODIFYIP"  property="modifyip" jdbcType="VARCHAR" javaType="String"/>
<result column="T_MODIFYTIME"  property="modifytime" jdbcType="DATE" javaType="Date"/>
<result column="KSDD"  property="ksdd" jdbcType="VARCHAR" javaType="String"/>
<result column="C_KSDDNAME"  property="ksddname" jdbcType="VARCHAR" javaType="String"/>
<result column="LSH"  property="lsh" jdbcType="VARCHAR" javaType="String"/>
<result column="SFZMHM"  property="sfzmhm" jdbcType="VARCHAR" javaType="String"/>
<result column="XM"  property="xm" jdbcType="VARCHAR" javaType="String"/>
 </resultMap>


<select id="select" resultType="ArrayList" resultMap="resultMap">
	 select m.* <include refid="joinColums"/> from <include refid="table" /> m <include refid="join"/> 
<trim prefix="where" prefixOverrides="and">
<if test="model.id neq null">and m.ID=#{model.id,jdbcType=INTEGER javaType=Integer } </if> 
<if test="model.createip neq null">and m.C_CREATEIP=#{model.createip,jdbcType=VARCHAR javaType=String } </if> 
<if test="model.createtime neq null">and to_char(m.T_CREATETIME,'YYYY-MM-DD')= to_char(#{model.createtime,jdbcType=DATE javaType=Date },'YYYY-MM-DD') </if> 
<if test="model.creator neq null">and m.C_CREATOR=#{model.creator,jdbcType=VARCHAR javaType=String } </if> 
<if test="model.isdeleted neq null">and m.I_ISDELETED=#{model.isdeleted,jdbcType=INTEGER javaType=Integer } </if> 
<if test="model.description neq null">and m.C_DESCRIPTION=#{model.description,jdbcType=VARCHAR javaType=String } </if> 
<if test="model.keywords neq null">and m.C_KEYWORDS=#{model.keywords,jdbcType=VARCHAR javaType=String } </if> 
<if test="model.modifier neq null">and m.C_MODIFIER=#{model.modifier,jdbcType=VARCHAR javaType=String } </if> 
<if test="model.modifyip neq null">and m.C_MODIFYIP=#{model.modifyip,jdbcType=VARCHAR javaType=String } </if> 
<if test="model.modifytime neq null">and to_char(m.T_MODIFYTIME,'YYYY-MM-DD')= to_char(#{model.modifytime,jdbcType=DATE javaType=Date },'YYYY-MM-DD') </if> 
<if test="model.ksdd neq null">and m.KSDD=#{model.ksdd,jdbcType=VARCHAR javaType=String } </if> 
<if test="model.ksddname neq null">and m.C_KSDDNAME=#{model.ksddname,jdbcType=VARCHAR javaType=String } </if> 
<if test="model.lsh neq null">and m.LSH=#{model.lsh,jdbcType=VARCHAR javaType=String } </if> 
<if test="model.sfzmhm neq null">and m.SFZMHM=#{model.sfzmhm,jdbcType=VARCHAR javaType=String } </if> 
<if test="model.xm neq null">and m.XM=#{model.xm,jdbcType=VARCHAR javaType=String } </if> 
</trim>
</select>
<select id="selectEqPk" parameterType="String"  resultMap="resultMap">
	 select <include refid="colums"/> from <include refid="table" /> m <include refid="join"/> where m.<include refid="pk" /> = #{id,jdbcType=INTEGER,javaType=Integer}
</select>

<select id="count" resultType="int">
	 select count(*) from <include refid="table"/>
</select>

<select id="selectInPk" resultType="ArrayList" resultMap="resultMap" >
	 select m.* <include refid="joinColums"/> from <include refid="table" /> m <include refid="join"/> where m.<include refid="pk" /> in
<foreach item="pk" collection="list" open="(" separator="," close=")"> #{pk} </foreach> 
<include refid="orderby"/>
</select>

<select id="selectNotInPk" resultType="ArrayList" resultMap="resultMap" >
	 select m.* <include refid="joinColums"/> from <include refid="table" /> m <include refid="join"/> where m.<include refid="pk" /> not in
<foreach item="pk" collection="list" open="(" separator="," close=")"> #{pk} </foreach> 
<include refid="orderby"/>
</select>

<select  id="selectAll"  resultType="ArrayList"  resultMap="resultMap">
	 select m.* <include refid="joinColums"/> from  <include refid="table" /> m <include refid="join"/> 
 <include refid="orderby"/>
</select>

<insert id="insertOne" >
	 insert into <include refid="table"/> ( <include refid="colums"/> ) values ( <include refid="insertCols"/>) 
</insert>

<update id="updateOne" flushCache="true" >
	 update <include refid="table"/> set <include refid="updateCols"/> where <include refid="pk" />=#{model.id,jdbcType=INTEGER,javaType=Integer}
</update>

<delete id="deleteInPk" flushCache="true">
	 delete from <include refid="table"/> where <include refid="pk" /> in
<foreach item="pk" collection="list" open="(" separator="," close=")">#{pk}</foreach>
</delete>

<delete id="deleteNotInPk" flushCache="true">
	 delete from <include refid="table"/> where <include refid="pk" /> not in
<foreach item="pk" collection="list" open="(" separator="," close=")">#{pk}</foreach>
</delete>



</mapper>